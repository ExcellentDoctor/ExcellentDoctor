<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tpkd.common.mapper.DepartmentMapper" >
    <select id="selectById" resultType="com.tpkd.common.pojo.Department">
        SELECT `departmentId`,`departmentName` FROM `department` WHERE `departmentId`=#{departmentId}
    </select>
</mapper>